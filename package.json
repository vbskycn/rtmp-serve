{
  "name": "stream-proxy",
  "version": "1.0.0",
  "description": "Stream proxy server",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "docker:build": "docker buildx build --platform linux/amd64,linux/arm64 -t zhoujie218/rtmp-serve:latest --push .",
    "docker:run": "docker run -d -p 3000:3000 -v $(pwd)/config:/app/config -v $(pwd)/streams:/app/streams -v $(pwd)/logs:/app/logs --name rtmp-serve zhoujie218/rtmp-serve:latest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pinyin": "^2.11.2",
    "winston": "^3.3.3",
    "fluent-ffmpeg": "^2.1.2",
    "axios": "^1.6.7",
    "cookie-parser": "^1.4.6",
    "jsonwebtoken": "^9.0.0",
    "pm2": "^5.3.0"
  }
}
